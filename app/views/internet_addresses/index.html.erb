<h1>Listing internet_addresses</h1>

<table>
  <tr>
    <th>Version</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @internet_addresses.each do |internet_address| %>
  <tr>
    <td><%= internet_address.version %></td>
    <td><%= internet_address.address %></td>
    <td><%= link_to 'Show', internet_address %></td>
    <td><%= link_to 'Edit', edit_internet_address_path(internet_address) %></td>
    <td><%= link_to 'Destroy', internet_address, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Internet address', new_internet_address_path %>
